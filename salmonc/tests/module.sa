mod test {
    var variable = 1;
    variable = variable + 1;
    var variable1 = "eeeh?";

    fun test() {
        print "hola";
    }

    class TestClass {
        testmethod() {
            print "hola method";
            print variable;
        }
    }

    trait TestTrait {
        echo(a);
    }

    fun test1() {
